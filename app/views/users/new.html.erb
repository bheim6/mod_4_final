<h1>Signup!</h1>

<%= form_for @user do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>

  <%= f.submit "Submit" %>
<% end %>
<p>
  <%= link_to "Already have account? click here to Login",  login_path %>
</p>
